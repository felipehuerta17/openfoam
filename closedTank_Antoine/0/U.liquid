FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottom
    {
        type            noSlip;
    }

    top
    {
        type            noSlip;
    }

    tankWall
    {
        type            noSlip;
    }

    "(front|back)"
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }

}

