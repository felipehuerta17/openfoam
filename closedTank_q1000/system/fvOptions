FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

buoyancy_gas
{
    type            buoyancyForce;
    selectionMode   all;
    fields (U.gas);
}

buoyancy_liquid
{
    type            buoyancyForce;
    selectionMode   all;
    fields (U.liquid);
}


