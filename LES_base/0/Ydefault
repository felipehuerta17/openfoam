FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ydefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    "(bottom.*)"
    {
        type            zeroGradient;
    }

    // Roof, closed
    "(roof.*)"
    {
        type            zeroGradient;
    }

    outlet
    {
        type            inletOutlet;
        phi             phi.gas;
        inletValue      $internalField;
        value           $internalField;
    }
    tank_wall
    {
        type            zeroGradient;
    }

    "(wedge.*)"
    {
        type            wedge;
    }
}

// ************************************************************************* //
